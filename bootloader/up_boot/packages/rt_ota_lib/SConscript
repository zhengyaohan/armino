from building import *
import rtconfig

cwd = GetCurrentDir()

src = ['']

path = [cwd + '/inc']

libs = ['']

libpath = [cwd + '/libs']

if rtconfig.CROSS_TOOL == 'keil':
    libs += ['librt_ota_0.1.2_arm968e-s_keil.lib']

group = DefineGroup('rt_ota', src, depend = ['PKG_USING_RT_OTA'], CPPPATH = path, LIBS = libs, LIBPATH = libpath)

Return('group')
