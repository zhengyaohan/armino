#
# Compile/Build
#
CONFIG_TOOLCHAIN_PATH="/opt/risc-v/nds32le-elf-mculib-v5/bin"

#CONFIG_MATTER_START=y
CONFIG_MBEDTLS=y
CONFIG_LWIP_V2_1=y
CONFIG_IPV6=y

CONFIG_SUPPORT_MATTER=y
CONFIG_MATTER_APP="lighting-app"
#CONFIG_MATTER_APP="thermostat"
#CONFIG_MATTER_APP="temperature-measurement-app"
#CONFIG_MATTER_APP="light-switch-app"

CONFIG_ENABLE_WIFI_DEFAULT_CONNECT=n
CONFIG_AT_CMD=n
CONFIG_HTTP=n
CONFIG_ALI_MQTT=n

#
# Video transfer
#
CONFIG_JPEG_ENCODE=n
CONFIG_CAMERA=n
CONFIG_USE_APP_DEMO_VIDEO_TRANSSER=n
CONFIG_APP_DEMO_VIDEO_TRANSFER=n
CONFIG_NET_WORK_VIDEO_TRANSFER=n
CONFIG_CAMERA_USE_I2C0=n
#
#end of Video transfer
